{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": ["ts-jest", {
      "tsconfig": {
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
      }
    }]
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(uuid)/)"
  ]
}
